n: int = int(input("Inserire il numero di neonati: "))

match n:

  #n=1  
    case 1:
        print("Congratulazioni!")
  #n=2  
    case 2:
        print("Wow! Gemelli")
  #n=3   
    case 3:
        print("Wow! Tre")
  #n=4  
    case 4:
        print("Mamma mia quattro! Wow!")
  #n=5  
    case 5:
        print("Incredibile cinque!")
  #n=x  
    case _:
        print(f"Non ci credo {n} bambini")